<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.Customer.CustomerAccountInformationFragment"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="-81dp">

    <TextView
        android:id="@+id/textView26"
        android:layout_width="380dp"
        android:layout_height="53dp"
        android:text="Account Information"
        android:textSize="40sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053" />

    <Button
        android:id="@+id/btnAccInfoBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:text="Back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoPassword"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Password"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoUsername"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoUsername"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoUsername" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoEmail"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Email"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoPassword"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoPassword"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoPassword" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoCCName"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Name on Card"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoEmail"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoEmail"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoEmail" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoUsername"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:editable="false"
        android:hint="Username"
        tools:layout_editor_absoluteX="189dp"
        tools:layout_editor_absoluteY="120dp" />

    <TextView
        android:id="@+id/textViewAccInfoCCNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Credit Card #:"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoCCNum"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoCardName"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCName" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoCCNum"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Credit Card #"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoCCName"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoCCName"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCName" />

    <TextView
        android:id="@+id/textViewAccInfoCCExpDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Exp Date:"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoCCExpDate"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoCCNum"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCNum" />

    <TextView
        android:id="@+id/textViewAccInfoCCCSV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CSV:"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoCCCSV"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoCCExpDate"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCExpDate" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoCCExpDate"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Exp Date"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoCCNum"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoCCNum"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCNum" />

    <TextView
        android:id="@+id/textViewAccInfoOrderHistory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Order History"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.737" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoCCCSV"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="CSV"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoCCExpDate"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoCCExpDate"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCExpDate" />

    <TextView
        android:id="@+id/textViewAccInfoPassword"
        android:layout_width="72dp"
        android:layout_height="25dp"
        android:text="Password:"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoPassword"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoUsername"
        app:layout_constraintTop_toTopOf="@+id/editTextAccInfoPassword" />

    <TextView
        android:id="@+id/textViewAccInfoEmail"
        android:layout_width="65dp"
        android:layout_height="23dp"
        android:text="Email:"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoEmail"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoPassword"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoPassword" />

    <TextView
        android:id="@+id/textViewAccInfoUsername"
        android:layout_width="75dp"
        android:layout_height="26dp"
        android:layout_marginEnd="24dp"
        android:text="Username:"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toTopOf="@+id/editTextAccInfoPassword"
        app:layout_constraintEnd_toStartOf="@+id/editTextAccInfoUsername"
        app:layout_constraintTop_toTopOf="@+id/editTextAccInfoUsername"
        app:layout_constraintVertical_bias="0.5" />

    <ScrollView
        android:id="@+id/scrollView6"
        android:layout_width="346dp"
        android:layout_height="113dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.569"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.896">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="9dp"
            tools:layout_editor_absoluteY="520dp">

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:clickable="true"
                android:text="Order 1"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:clickable="true"
                android:text="Order 2"
                android:textSize="20sp" />

        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/btnAccInfoSaveChanges"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Save Changes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.686" />

    <TextView
        android:id="@+id/textViewAccInfoCardName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name on Card:"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoCCName"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoEmail"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoEmail" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        android:layout_width="273dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="138dp" />

</androidx.constraintlayout.widget.ConstraintLayout>