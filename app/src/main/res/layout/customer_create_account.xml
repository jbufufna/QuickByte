<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.Customer.CustomerCreateAccountFragment">

    <TextView
        android:id="@+id/textView27"
        android:layout_width="336dp"
        android:layout_height="66dp"
        android:text="Create Account"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.08" />

    <Button
        android:id="@+id/btnCreateAccSignIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sign In"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias=".50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.951" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editText12"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Exp Year"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoCCExpDate"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoCCExpDate"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCExpDate" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoCCExpDate"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Exp Month"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoCCNum"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoCCNum"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCNum" />

    <TextView
        android:id="@+id/textViewAccInfoCCNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Credit Card #:"
        app:layout_constraintBottom_toTopOf="@+id/editTextAccInfoCCExpDate"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/editText3" />

    <TextView
        android:id="@+id/textViewAccInfoCCExpDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expiration Month:"
        app:layout_constraintBottom_toTopOf="@+id/editText12"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoCCNum"
        app:layout_constraintTop_toTopOf="@+id/editTextAccInfoCCExpDate" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoPassword"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Password"
        app:layout_constraintEnd_toEndOf="@+id/textInputPassword"
        app:layout_constraintStart_toStartOf="@+id/textInputPassword"
        app:layout_constraintTop_toBottomOf="@+id/textInputPassword" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoCCCSV"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="CSV"
        app:layout_constraintEnd_toEndOf="@+id/editText12"
        app:layout_constraintStart_toStartOf="@+id/editText12"
        app:layout_constraintTop_toBottomOf="@+id/editText12" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputPassword"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Email"
        app:layout_constraintEnd_toEndOf="@+id/textInputEditText"
        app:layout_constraintStart_toStartOf="@+id/textInputEditText"
        app:layout_constraintTop_toBottomOf="@+id/textInputEditText" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name on Card:"
        app:layout_constraintBottom_toBottomOf="@+id/editText3"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoPassword"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoPassword" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editText3"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Full Name"
        app:layout_constraintEnd_toEndOf="@+id/editTextAccInfoPassword"
        app:layout_constraintStart_toStartOf="@+id/editTextAccInfoPassword"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoPassword" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        app:layout_constraintBottom_toTopOf="@+id/editTextAccInfoPassword"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="@+id/textInputPassword" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/editTextAccInfoCCNum"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Credit Card #"
        app:layout_constraintEnd_toEndOf="@+id/editText3"
        app:layout_constraintStart_toStartOf="@+id/editText3"
        app:layout_constraintTop_toBottomOf="@+id/editText3" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expiration Year:"
        app:layout_constraintBottom_toBottomOf="@+id/editText12"
        app:layout_constraintEnd_toEndOf="@+id/textViewAccInfoCCExpDate"
        app:layout_constraintTop_toTopOf="@+id/editText12" />

    <TextView
        android:id="@+id/textViewAccInfoPassword"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password:"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoPassword"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/editTextAccInfoPassword" />

    <TextView
        android:id="@+id/textViewAccInfoOrderHistory"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CSV:"
        app:layout_constraintBottom_toBottomOf="@+id/editTextAccInfoCCCSV"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/editText12" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="68dp"
        android:layout_height="23dp"
        android:layout_marginEnd="16dp"
        android:text="Username:"
        app:layout_constraintBottom_toTopOf="@+id/textInputPassword"
        app:layout_constraintEnd_toStartOf="@+id/textInputEditText"
        app:layout_constraintTop_toTopOf="@+id/textInputEditText"
        app:layout_constraintVertical_bias=".5" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText"
        android:layout_width="194dp"
        android:layout_height="48dp"
        android:hint="Username"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.829"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />

    <Button
        android:id="@+id/btnCreateAccCreateAcc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Create Account"
        app:layout_constraintBottom_toTopOf="@+id/textViewAlreadyAccount"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextAccInfoCCCSV"
        app:layout_constraintVertical_bias="0.531" />

    <TextView
        android:id="@+id/textViewAlreadyAccount"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:text="Already have an account?"
        app:layout_constraintBottom_toTopOf="@+id/btnCreateAccSignIn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.987" />

</androidx.constraintlayout.widget.ConstraintLayout>